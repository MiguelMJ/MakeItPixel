/**
 * Example MIP program
 */
 
img = load(_1)
p = closest_rgb(#0 =<3>=> #ffffff)
d1 = dither_ord("Bayes4", 1)
d2 = dither_ord("Bayes4", 3)
d3 = dither_ord("Bayes4", 7)

push("img")
quantize(img, p, d1)
save(img, "bayes4_1.png")

undo("img")
quantize(img, p, d2)
save(img, "bayes4_3.png")

pop("img")
quantize(img, p, d3)
save(img, "bayes4_7.png")
