img = load("monalisa.png")
p = closest_rgb([#ffffff, #000000, #101010])
d1 = dither_ord("Bayes4", 1)
d2 = dither_ord("Bayes4", 3)
d3 = dither_ord("Bayes4", 7)
push("img")
quantize(img, p, d1)
save(img, "mona1.png")
pop("img")
push("img")
quantize(img, p, d2)
save(img, "mona2.png")
pop("img")
quantize(img, p, d3)
save(img, "mona3.png")
